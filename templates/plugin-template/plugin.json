{
  "id": "my-asyncapi-plugin",
  "name": "My AsyncAPI Plugin",
  "version": "1.0.0",
  "description": "Custom TypeSpec AsyncAPI plugin for integrating with My Service messaging platform",
  "author": {
    "name": "Your Name",
    "email": "you@example.com",
    "url": "https://yourwebsite.com"
  },
  "license": "MIT",
  "keywords": [
    "typespec",
    "asyncapi",
    "messaging",
    "event-driven",
    "my-service",
    "cloud-messaging"
  ],
  "category": "cloud-provider",
  "maturity": "beta",
  "supportedAsyncApiVersions": ["3.0.0"],
  "typespecVersion": ">=0.50.0",
  "homepage": "https://github.com/yourusername/my-asyncapi-plugin",
  "repository": "https://github.com/yourusername/my-asyncapi-plugin",
  "documentation": "https://github.com/yourusername/my-asyncapi-plugin/blob/main/docs/README.md",
  "installation": {
    "package": "my-asyncapi-plugin",
    "command": "npm install my-asyncapi-plugin",
    "setup": [
      "Configure service endpoint and credentials",
      "Add plugin to TypeSpec configuration",
      "Import plugin in your TypeSpec files"
    ]
  },
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "endpoint": {
        "type": "string",
        "format": "uri",
        "description": "Service endpoint URL"
      },
      "timeout": {
        "type": "integer",
        "minimum": 0,
        "maximum": 300000,
        "description": "Request timeout in milliseconds"
      },
      "retries": {
        "type": "integer",
        "minimum": 0,
        "maximum": 10,
        "description": "Number of retry attempts"
      },
      "auth": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": ["api-key", "bearer-token", "oauth", "basic"]
          },
          "credentials": {
            "type": "object"
          }
        },
        "required": ["type", "credentials"]
      },
      "rateLimit": {
        "type": "object",
        "properties": {
          "requestsPerSecond": {
            "type": "integer",
            "minimum": 1,
            "maximum": 10000
          },
          "burstLimit": {
            "type": "integer",
            "minimum": 1
          }
        }
      },
      "messageFormat": {
        "type": "object",
        "properties": {
          "encoding": {
            "type": "string",
            "enum": ["json", "avro", "protobuf", "xml"]
          },
          "validateSchema": {
            "type": "boolean"
          },
          "compression": {
            "type": "string",
            "enum": ["gzip", "lz4", "snappy"]
          }
        }
      }
    },
    "required": ["endpoint"]
  },
  "capabilities": {
    "protocols": ["http", "https", "websocket"],
    "messageFormats": ["json", "avro", "protobuf", "xml"],
    "authenticationTypes": ["api-key", "bearer-token", "oauth", "basic"],
    "features": [
      "request-response",
      "publish-subscribe",
      "message-transformation",
      "rate-limiting",
      "retry-logic",
      "schema-validation",
      "compression-support"
    ],
    "limitations": {
      "maxMessageSize": 10485760,
      "maxRetries": 10,
      "maxTimeout": 300000,
      "maxRateLimit": 10000
    }
  },
  "examples": [
    {
      "name": "Basic Usage",
      "description": "Simple service binding with endpoint configuration",
      "code": "@bindings(\"my-service\", {\n  endpoint: \"https://api.myservice.com\"\n})\n@publish\nop sendMessage(): MyMessage;"
    },
    {
      "name": "Advanced Configuration",
      "description": "Full configuration with authentication and rate limiting",
      "code": "@bindings(\"my-service\", {\n  endpoint: \"https://api.myservice.com\",\n  timeout: 10000,\n  retries: 3,\n  auth: {\n    type: \"api-key\",\n    credentials: { key: \"my-api-key\" }\n  },\n  rateLimit: {\n    requestsPerSecond: 100,\n    burstLimit: 200\n  },\n  messageFormat: {\n    encoding: \"json\",\n    validateSchema: true,\n    compression: \"gzip\"\n  }\n})\n@subscribe\nop receiveEvents(): EventMessage;"
    },
    {
      "name": "OAuth Authentication",
      "description": "Using OAuth2 for secure service access",
      "code": "@bindings(\"my-service\", {\n  endpoint: \"https://api.myservice.com\",\n  auth: {\n    type: \"oauth\",\n    credentials: {\n      clientId: \"your-client-id\",\n      clientSecret: \"your-client-secret\",\n      scope: \"messaging:read messaging:write\"\n    }\n  }\n})\n@publish\nop publishEvent(): EventMessage;"
    }
  ],
  "dependencies": [
    "effect"
  ],
  "peerDependencies": [
    "@typespec/compiler",
    "@typespec/asyncapi"
  ],
  "minimumSystemVersion": "1.0.0",
  "checksum": "sha256:placeholder-checksum-will-be-generated-during-build",
  "downloadCount": 0,
  "rating": 0,
  "lastUpdated": "2025-01-01T00:00:00.000Z",
  "created": "2025-01-01T00:00:00.000Z"
}